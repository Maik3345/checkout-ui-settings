{
  "name": "vite-preact-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch --emptyOutDir",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx"
  },
  "dependencies": {
    "jotai": "^2.5.1",
    "preact": "^10.16.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.3",
    "@types/jest": "^29.5.7",
    "@types/jquery": "^3.5.25",
    "happy-dom": "^18.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^19.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.8",
    "vitest": "^3.2.4"
  }
}
